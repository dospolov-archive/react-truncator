{
  "name": "react-truncator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:open:win": "cross-env SYSTEMROOT='C:/Windows' vite --open",
    "start": "vite",
    "start:open": "cross-env vite --open",
    "build": "cross-env CI=false && vite build",
    "test": "npm run lint",
    "lint": "eslint src --ext .js,.jsx",
    "lint-fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier src --write \"**/*.{js,jsx}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.14",
    "@headlessui/react": "^1.7.7",
    "@neupauer/tailwindcss-plugin-separated": "^1.0.0",
    "@tippyjs/react": "^4.2.6",
    "just-filter-object": "^3.2.0",
    "just-reduce-object": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-icons": "^4.7.1",
    "react-inlinesvg": "^3.0.1",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.6.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "^7.12.13",
